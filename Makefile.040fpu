CC := m68k-amigaos-gcc
LD := m68k-amigaos-ld
CFLAGS += -g -m68040 -m68881
LDFLAGS += -noixemul

export CFLAGS LDFLAGS

all: lib module

lib: FORCE
	$(MAKE) -f ../../Lib/Makefile

module: lib FORCE
	$(MAKE) -f ../../module/Makefile VFlash_68040fpu.module

clean:
	$(MAKE) -f ../../Lib/Makefile clean
	$(MAKE) -f ../../module/Makefile clean

FORCE:

